@{
    ViewBag.Title = "Listado de productos";
}

<h2>Listado de productos</h2>

<p>@Html.Raw(TempData["msg"])</p>

<div class="well well-sm">
    <a href="@Href("~/product/add")" class="btn btn-success">Agregar</a>
</div>

<ul>
    @foreach(Inventory.Product product in ViewBag.Products) {
    <li>@product.Name</li>
    }
</ul>

