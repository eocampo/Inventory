<!DOCTYPE html>
<html>
  <head>
    <title>Facturación</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" >
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="@Html.Raw(System.Web.VirtualPathUtility.ToAbsolute("~/Content/bootstrap/bootstrap.min.css"))" rel="stylesheet" media="screen">
    
    <style>
    body {
      padding-top: 60px;
    }
    </style>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="@Html.Raw(System.Web.VirtualPathUtility.ToAbsolute("~/Content/bootstrap/assets/js/html5shiv.js"))"></script>
      <script src="@Html.Raw(System.Web.VirtualPathUtility.ToAbsolute("~/Content/bootstrap/assets/js/respond.min.js"))"></script>
    <![endif]-->
  </head>
  <body>

    @Html.Partial("_MainMenuPartial")

    <div class="container">

        @RenderBody()

    </div><!-- /.container -->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="@Html.Raw(System.Web.VirtualPathUtility.ToAbsolute("~/Content/jquery/jquery.js"))"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="@Html.Raw(System.Web.VirtualPathUtility.ToAbsolute("~/Content/bootstrap/bootstrap.min.js"))"></script>

    <div class="scripts">
        @RenderSection("pageScripts", required: false)
    </div>

  </body>
</html>